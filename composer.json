{
    "name": "dakatsuka/blueprint-bundle",
    "type": "symfony-bundle",
    "description": "The bundle provides a way to manage test data for the Doctrine ORM",
    "keywords": ["blueprint", "test", "symfony2"],
    "license": "MIT",
    "authors": [
        {
            "name": "Dai Akatsuka",
            "email": "d.akatsuka@gmail.com",
            "homepage": "https://github.com/dakatsuka/BlueprintBundle"
        }
    ],
    "require": {
        "php": ">=5.3.2",
        "doctrine/orm": ">=2.2.3,<2.4-dev"
    },
    "require-dev": {
        "mockery/mockery": "dev-master@dev"
    },
    "autoload": {
        "psr-0": { "Dakatsuka\\BlueprintBundle": "" }
    },
    "target-dir": "Dakatsuka/BlueprintBundle",
    "minimum-stability": "stable"
}
